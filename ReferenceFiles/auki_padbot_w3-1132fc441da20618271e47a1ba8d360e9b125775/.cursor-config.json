{
    "project_info": {
        "name": "auki_padbot_w3",
        "description": "Robot GUI project with Cactus and GoTu variants"
    },
    "build_instructions": {
        "android": {
            "command": "cd RobotGUI/android && ./gradlew clean && ./gradlew assembleRelease",
            "output_paths": {
                "cactus": "RobotGUI/android/app/build/outputs/apk/cactus/release/Cactus.apk",
                "gotu": "RobotGUI/android/app/build/outputs/apk/gotu/release/GoTu.apk"
            }
        }
    },
    "important_paths": {
        "android_root": "RobotGUI/android",
        "app_src": "RobotGUI/android/app/src",
        "gradle_root": "RobotGUI/android",
        "react_src": "RobotGUI/src",
        "source_structure": {
            "android": {
                "main": "RobotGUI/android/app/src/main",
                "cactus": "RobotGUI/android/app/src/cactus",
                "gotu": "RobotGUI/android/app/src/gotu",
                "debug": "RobotGUI/android/app/src/debug"
            },
            "react": {
                "utils": "RobotGUI/src/utils",
                "cactus": "RobotGUI/src/cactus",
                "gotu": "RobotGUI/src/gotu"
            }
        }
    },
    "version_control": {
        "branch": "main",
        "remote": "origin"
    },
    "key_functions": {
        "health_check": {
            "interval": 60000,
            "locations": {
                "main_screen": "RobotGUI/src/gotu/screens/MainScreen.tsx",
                "config_screen": "RobotGUI/src/gotu/screens/ConfigScreen.tsx"
            }
        },
        "navigation": {
            "main_screen": "RobotGUI/src/gotu/screens/MainScreen.tsx",
            "native_module": "RobotGUI/android/app/src/main/java/com/auki/padbot/SlamtecUtilsModule.java"
        },
        "authentication": {
            "domain_utils": "RobotGUI/android/app/src/main/java/com/auki/padbot/DomainUtilsModule.java",
            "config_screen": "RobotGUI/src/gotu/screens/ConfigScreen.tsx"
        }
    },
    "logging": {
        "debug_log": "debug_log.txt",
        "log_utils": "RobotGUI/src/utils/LogUtils.ts"
    },
    "robot_control": {
        "base_url": "http://127.0.0.1:1448",
        "api_endpoints": {
            "navigation": "/api/core/motion/v1/actions",
            "pose": "/api/core/pose/v1/pose"
        }
    },
    "behavioral_rules": {
        "code_changes": {
            "require_approval": true,
            "no_assumptions": true,
            "check_files_in_detail": true
        },
        "logging": {
            "health_check": {
                "required": true,
                "interval": 60000,
                "log_full_response": true
            },
            "navigation": {
                "log_attempts": true,
                "log_errors": true,
                "log_success": true
            }
        },
        "error_handling": {
            "log_all_errors": true,
            "retry_mechanisms": {
                "navigation": true,
                "authentication": true
            }
        }
    }
} 